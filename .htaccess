# .htaccess para diagnóstico UOL Host
DirectoryIndex diagnostico-uolhost.php index.php

# Aumentar limites
php_value max_execution_time 60
php_value memory_limit 256M

# Habilitar logs de erro
php_flag log_errors On
php_value error_log "php_errors.log"

# Permissões básicas
Options +FollowSymLinks
Options -Indexes

# Sem reescrita por enquanto
RewriteEngine Off
